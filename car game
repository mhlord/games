command =""
started = False
while True:
      command = input(">").lower()
      if command == "start":
          if started:
              print("car started...")
          else:
              started = True
          print (" car already started")
      elif command == "stop":
          print("car stopped...")
          if not started:
                print (" car already stopped")
          else:
              started = False
      elif command == "help":
          print("""
quit-finish the game          
start- to start the car
stop- to stop the car""")
      elif command == "quit":
          break
      else:
          print( "sorry i donÂ´t understand that")
